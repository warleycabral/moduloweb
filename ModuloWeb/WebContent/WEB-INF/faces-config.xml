<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
     <application>
	<locale-config>
		<default-locale>pt_BR</default-locale>
		<supported-locale>pt_BR</supported-locale>
		<supported-locale>en_US</supported-locale>
	</locale-config>
		<resource-bundle>
			<base-name>br.com.sistxweb.i18n.mensagens</base-name>
			<var>lang</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>csu01Bean</managed-bean-name>
		<managed-bean-class>br.com.sistxweb.bean.csu01Bean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>csu04Bean</managed-bean-name>
		<managed-bean-class>br.com.sistxweb.bean.csu04Bean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>csu11Bean</managed-bean-name>
		<managed-bean-class>br.com.sistxweb.bean.csu11Bean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>monitorBean</managed-bean-name>
		<managed-bean-class>br.com.sistxweb.bean.monitorBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contextoBean</managed-bean-name>
		<managed-bean-class>br.com.sistxweb.bean.ContextoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
</faces-config>
